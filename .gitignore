}
#chat .mention {
    background-color: rgba(129, 118, 255, 0.22) !important;
    font-weight: 700;
    padding-left: 25px;
    border-left: #7681FF 3px solid;
}
.emote.from-515739d7c3b97a1ae280ace8 .from.staff.clickable {
    display: none;
}
#chat .emote {
    border-left: #FF0000 3px solid;
